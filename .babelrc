{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                }
            }
        ],
        "stage-2"
    ],
    "sourceType": "module",
    "plugins": [
        "transform-vue-jsx",
        "transform-runtime",
        "transform-es2015-modules-commonjs",
        [
            "import",
            {
                "libraryName": "ant-design-vue",
                "libraryDirectory": "es",
                "style": true
            }
        ]
    ]
}
